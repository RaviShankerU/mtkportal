          <Chart name="campaign_type" title="Campaign by Type" type="0" height="400" width="6" domainColumnName="Campaign Type" domainColumnLabel="Campaign Type" domainColumnType="string">
            <DataColumns>
              <Column name="COUNT" label="Campaign Type" type="int" annotationColumnName="Campaign Type" annotationTextColumnName="Campaign Type"/>
            </DataColumns>
            <SelectStatement>
SELECT 
	(CASE
		WHEN `channel_types` LIKE '%Tradeshows Campaign%' THEN 'Tradeshows Campaign'
		WHEN `channel_types` LIKE '%Live Events%' THEN 'Live Events Campaign'
 		WHEN `channel_types` LIKE '%Webinar%' THEN 'Webinar'
 		WHEN `channel_types` LIKE '%eBook/Whitepaper%' THEN 'eBook / Whitepaper Campaign'
 		WHEN `channel_types` LIKE '%Website Event List%' THEN 'Website Event List'
 		WHEN `channel_types` LIKE '%Social Campaign%' THEN 'Social Campaign'
 		WHEN `channel_types` LIKE '%Social (Paid)%' THEN 'Social (Paid)'
 		WHEN `channel_types` LIKE '%Marketo Campaign%' THEN 'Marketo Campaign'
 		WHEN `channel_types` LIKE '%R Campaign%' THEN 'PR Campaign'
 		WHEN `channel_types` LIKE '%Webinar Series Campaign%' THEN 'Webinar Series Campaign'
        ELSE 'No Data or Incorrect'
        END) as 'Campaign Type',      
        COUNT(*) as 'COUNT' 
FROM `brief`
GROUP By (CASE
		WHEN `channel_types` LIKE '%Tradeshows Campaign%' THEN 'Tradeshows Campaign'
		WHEN `channel_types` LIKE '%Live Events%' THEN 'Live Events Campaign'
 		WHEN `channel_types` LIKE '%Webinar%' THEN 'Webinar'
 		WHEN `channel_types` LIKE '%eBook/Whitepaper%' THEN 'eBook / Whitepaper Campaign'
 		WHEN `channel_types` LIKE '%Website Event List%' THEN 'Website Event List'
 		WHEN `channel_types` LIKE '%Social Campaign%' THEN 'Social Campaign'
 		WHEN `channel_types` LIKE '%Social (Paid)%' THEN 'Social (Paid)'
 		WHEN `channel_types` LIKE '%Marketo Campaign%' THEN 'Marketo Campaign'
 		WHEN `channel_types` LIKE '%R Campaign%' THEN 'PR Campaign'
 		WHEN `channel_types` LIKE '%Webinar Series Campaign%' THEN 'Webinar Series Campaign'
        ELSE 'No Data or Incorrect'
        END)
ORDER BY (CASE
		WHEN `channel_types` LIKE '%Tradeshows Campaign%' THEN 'Tradeshows Campaign'
		WHEN `channel_types` LIKE '%Live Events%' THEN 'Live Events Campaign'
 		WHEN `channel_types` LIKE '%Webinar%' THEN 'Webinar'
 		WHEN `channel_types` LIKE '%eBook/Whitepaper%' THEN 'eBook / Whitepaper Campaign'
 		WHEN `channel_types` LIKE '%Website Event List%' THEN 'Website Event List'
 		WHEN `channel_types` LIKE '%Social Campaign%' THEN 'Social Campaign'
 		WHEN `channel_types` LIKE '%Social (Paid)%' THEN 'Social (Paid)'
 		WHEN `channel_types` LIKE '%Marketo Campaign%' THEN 'Marketo Campaign'
 		WHEN `channel_types` LIKE '%R Campaign%' THEN 'PR Campaign'
 		WHEN `channel_types` LIKE '%Webinar Series Campaign%' THEN 'Webinar Series Campaign'
        ELSE 'No Data or Incorrect'
        END) DESC            </SelectStatement>
          </Chart>
          <Chart name="campaign_participation" title="Campaign by Participation" type="2" height="400" width="6" domainColumnName="Business Name" domainColumnLabel="Campaign Participation" domainColumnType="string" visible="false">
            <DataColumns>
              <Column name="Business Name" label="Business" type="float"/>
              <Column name="Leading" label="Leading" type="int"/>
              <Column name="Attending" label="Attending" type="int"/>
            </DataColumns>
            <SelectStatement>
SELECT (CASE WHEN `Business_Responsible` LIKE '%MSC%' THEN 'MSC Software'
             WHEN `Business_Responsible` LIKE '%Cradle%' THEN 'Cradle'
             WHEN `Business_Responsible` LIKE '%Simufact%' THEN 'Simufact'
             WHEN `Business_Responsible` LIKE '%FFT%' THEN 'FFT'
             WHEN `Business_Responsible` LIKE '%FTI%' THEN 'FTI'
             WHEN `Business_Responsible` LIKE '%e-Xstream%' THEN 'e-Xstream'
             WHEN `Business_Responsible` LIKE '%Amendate%' THEN 'Amendate'
             WHEN `Business_Responsible` LIKE '%Vires%' THEN 'Vires'
             WHEN `Business_Responsible` LIKE '%Hexagon%' THEN 'Hexagon'
        	 ELSE 'Other'
        END) as 'Business Name', COUNT(`Business_Responsible`) As 'Leading', Count(`Brands_Attending`) As `Attending`
FROM brief
GROUP by (CASE WHEN `Business_Responsible` LIKE '%MSC%' THEN 'MSC Software'
             WHEN `Business_Responsible` LIKE '%Cradle%' THEN 'Cradle'
             WHEN `Business_Responsible` LIKE '%Simufact%' THEN 'Simufact'
             WHEN `Business_Responsible` LIKE '%FFT%' THEN 'FFT'
             WHEN `Business_Responsible` LIKE '%FTI%' THEN 'FTI'
             WHEN `Business_Responsible` LIKE '%e-Xstream%' THEN 'e-Xstream'
             WHEN `Business_Responsible` LIKE '%Amendate%' THEN 'Amendate'
             WHEN `Business_Responsible` LIKE '%Vires%' THEN 'Vires'
             WHEN `Business_Responsible` LIKE '%Hexagon%' THEN 'Hexagon'
        	 ELSE 'Other'
          END)
ORDER BY COUNT(*) DESC            </SelectStatement>
