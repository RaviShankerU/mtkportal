            <DataColumns>
              <Column name="COUNT" label="Country" type="int" annotationColumnName="Country_Name"/>
            </DataColumns>
            <SelectStatement>
SELECT 
    t2.Country_Name,
    COUNT(*) as 'COUNT'
FROM
    brief t1
