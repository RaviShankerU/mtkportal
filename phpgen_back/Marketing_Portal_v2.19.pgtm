 		WHEN `Industry` LIKE '%Aero%'  or  `Industry` LIKE '%Aero%'   THEN 'Aero'
		WHEN `Industry` LIKE '%all%' THEN 'Includes All Industries'
 		WHEN `Industry` LIKE '%AM%' or  `Industry` LIKE '%Auot%' or  `Industry` LIKE '%Auto%'  THEN 'Automotive'
